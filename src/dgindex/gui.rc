        BEGIN
            POPUP "Dynamic Range Control"
            BEGIN
                MENUITEM "Off",                         IDM_DRC_NONE
                MENUITEM SEPARATOR
                MENUITEM "Light",                       IDM_DRC_LIGHT
                MENUITEM "Normal",                      IDM_DRC_NORMAL
